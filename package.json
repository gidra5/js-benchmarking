{
  "name": "bench-js",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "bench": "vitest bench",
    "test": "vitest run",
    "build": "tsc || exit 0",
    "start": "npm run build && node ./dist/index.js",
    "watch": "vitest --ui",
    "compile": "bun run compile-win && bun run compile-linux",
    "compile-win": "bun build ./src/index.ts --compile --minify --target=bun-windows-x64 --sourcemap --outfile ./dist/miniUni.exe",
    "compile-linux": "bun build ./src/index.ts --compile --minify --sourcemap --outfile ./dist/miniUni"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@fast-check/vitest": "^0.1.3",
    "@types/node": "^22.8.1",
    "@types/react": "^18.3.12",
    "@vitest/coverage-istanbul": "^2.1.3",
    "@vitest/ui": "^2.1.3",
    "fast-check": "^3.22.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vite-node": "^2.1.3",
    "vitest": "^2.1.3"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^4.3.3",
    "ink": "^5.0.1",
    "react": "^18.3.1"
  }
}
